{
  "name": "Lead Synth",
  "description": "Lead synthesizer with arpeggiator and delay",
  "category": "synth",
  "version": "1.0",
  "created_at": "2025-10-03T14:30:00Z",
  "author": "MidiMind Team",
  
  "routing": {
    "input_device": "USB MIDI Keyboard",
    "output_device": "USB MIDI Synth",
    "channel": 1
  },
  
  "processors": [
    {
      "type": "transpose",
      "enabled": true,
      "params": {
        "semitones": 12
      }
    },
    {
      "type": "velocity",
      "enabled": true,
      "params": {
        "scale": 1.2,
        "offset": 10
      }
    },
    {
      "type": "arpeggiator",
      "enabled": true,
      "params": {
        "pattern": "up",
        "rate": "1/16",
        "octaves": 2,
        "gate": 0.8
      }
    },
    {
      "type": "delay",
      "enabled": true,
      "params": {
        "delay_time_ms": 250,
        "feedback": 0.5,
        "mix": 0.3
      }
    }
  ],
  
  "midi_settings": {
    "velocity_curve": "linear",
    "note_priority": "last",
    "sustain_pedal": true
  }
}